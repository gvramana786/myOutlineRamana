{

  "description": "Adds browser action icon to toolbar to open packaged web page.",
  "manifest_version": 2,
  "name": "myOutlineRamana",
  "version": "1.0",
  "homepage_url": "https://github.com/gvramana786/myOutlineRamana",
  "icons": {
    "48": "icons/page-48.png"
  },

   "permissions": [
   "tabs",
    "activeTab"
  ],
  
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/page-32.png"
  }
  
}
